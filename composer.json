{
    "name": "emjones/php-docker-compose-skeleton",
    "description": "Skeleton for PHP libraries, packages and components including base for docker running",
    "license": "MIT",
    "authors": [
        {
            "name": "Em Jones",
            "email": "em@agiantagravic.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Petk\\Skeleton\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-update-cmd": "Petk\\Installer::postUpdate",
        "post-package-install": [
            "Petk\\Installer::postPackageInstall"
        ],
        "post-install-cmd": [
            "Petk\\Installer::warmCache",
            "phpunit"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    }
}
